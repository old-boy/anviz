<section class="invoice users-table">
  <div class="col-xs-12">
    <h2 class="page-header">
      <i class="fa fa-globe"></i> Anviz, Inc.
      <small class="pull-right">{{curDate}}</small>
    </h2>
  </div>
  <div class="row">
        <div class="col-xs-12 table-responsive">
          <table class="table table-striped">
            <thead>
            <tr>
              <th>Id</th>
              <th>fullName</th>
              <th>email</th>
              <th>password</th>
              <th>date</th>
            </tr>
            </thead>
            <tbody>
              {{#each users}}
                <tr>
                  <td>{{_id}}</td>
                  <td>{{fullName}}</td>
                  <td>{{email}}</td>
                  <td>{{password}}</td>
                  <td>{{date}}</td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
</section>